{
    "name": "convochatsms/laravel-sms-whatsapp-gateway",
    "description": "Laravel package for ConvoChat SMS and WhatsApp gateway integration",
    "keywords": ["laravel", "sms", "whatsapp", "notifications", "convochat", "gateway"],
    "homepage": "https://github.com/RenatoAscencio/convochatsms",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ConvoChat Team",
            "email": "support@convo.chat",
            "homepage": "https://convo.chat"
        }
    ],
    "support": {
        "issues": "https://github.com/RenatoAscencio/convochatsms/issues",
        "source": "https://github.com/RenatoAscencio/convochatsms",
        "docs": "https://github.com/RenatoAscencio/convochatsms#readme"
    },
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-mockery": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "ConvoChat\\LaravelSmsGateway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConvoChat\\LaravelSmsGateway\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ConvoChat\\LaravelSmsGateway\\ConvoChatServiceProvider"
            ],
            "aliases": {
                "ConvoChat": "ConvoChat\\LaravelSmsGateway\\Facades\\ConvoChat"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
